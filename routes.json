{
"routes": [
    {
      "backend": {
        "type": "HTTP_BACKEND",
        "url": "http://{CI_PRIVATE_IP}:3000/${request.path[path]}"
      },
      "methods": [
        "ANY"
      ],
      "path": "/api/{path*}"
    },
    {
      "backend": {
        "type": "HTTP_BACKEND",
        "url": "http://{CI_PRIVATE_IP}:3000/docs/${request.path[path]}"
      },
      "methods": [
        "ANY"
      ],
      "path": "/docs/{path*}"
    },
    {
      "backend": {
        "body": null,
        "headers": [
          {
            "name": "Location",
            "value": "/docs/"
          }
        ],
        "status": 301,
        "type": "STOCK_RESPONSE_BACKEND"
      },
      "methods": [
        "ANY"
      ],
      "path": "/"
    }
  ]
}
